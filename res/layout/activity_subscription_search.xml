<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.treeapps.treenotes.sharing.subscriptions.ActivitySubscriptionSearch"
    tools:ignore="MergeRootFrame">
    
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >
        
        <TextView
            android:id="@+id/lblImageAnotationStatus"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Select from the following publications:"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <ListView
            android:id="@+id/android:list"
            android:layout_width="match_parent"
            android:layout_height="260dp"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/lblImageAnotationStatus" >

        </ListView>
        
    </RelativeLayout>

        
    
</FrameLayout>

